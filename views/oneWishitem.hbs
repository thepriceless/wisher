<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="keywords" content="wishlist, present, party, wish">
    <meta name="description" content="Your wishlist">
    {{> linkCommonCss}}
    <link href="/css/suggestion-zone/suggestion-zone.css" rel="stylesheet">
    <script src="/js/upload-wish-item.js"></script>
    <script src="/js/one-wishitem.js"></script>
    {{> linkCommonJs}}
    {{> websocketStatic}}
    <title>My wishlist</title>
</head>
<body>
    <div class="body__main">
        {{> navbarLoggedSure}}
        <main>
            <div class="suggestion-zone-with-buttons">
                <div class="suggestion-zone">
                    <h1 class="suggestion-zone__title" id="wisher-item-title">
                        {{ wishitem.title }}
                    </h1>
                    <h3 class="suggestion-zone__description" id="wisher-item-description">
                        {{ wishitem.description }}
                    </h3>
                    <div class="suggestion-zone__importance" id="suggestion-zone__importance">
                        <h4 class="helper-text">Importance</h4>
                        <div class="suggestion-zone__importance-stars">
                            {{#each (range 1 6)}}
                                {{#if (lte this ../wishitem.importance)}}
                                <img src="data:image/png;base64,">
                                {{else}}
                                <img src="data:image/png;base64,">
                                {{/if}}
                            {{/each}}
                        </div>
                    </div>
                    <div class="suggestion-zone__importance" id="suggestion-zone__marketplace-links">
                        <h4 class="helper-text">Marketplace<br>links</h4>
                        <div class="links-container">
                            {{#each wishitem.itemshopLinks}}
                                <a class="suggestion-zone__marketplace-single-link" href="{{this}}" target="_blank">Link â„–{{addOne @index}}</a>
                            {{/each}}
                        </div>
                    </div>
                    <img class="suggestion-zone__photo" id="wisher-item-image" src='{{ wishitem.imageLink }}' alt="image">
                </div>
                <div class="suggestion-zone__add-item-wrapper" id="wishitem-in-wishlist__add-item-wrapper">
                    <a class="suggestion-zone__add-this-wishitem-redirect" href="/wishitems/new?id={{ wishitem.id }}">
                        <img class="nav-bar__special-icon" src="/objects/icons8-plus.svg" alt="next"/>
                    </a>
                </div>
            </div>
        </main>
    </div>
    {{> footerAd}}
</body>
</html>